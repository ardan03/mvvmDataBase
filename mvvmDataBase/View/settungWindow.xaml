<Window x:Class="mvvmDataBase.View.settungWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mvvmDataBase.View"
        mc:Ignorable="d"
        Title="settungWindow" Height="450" Width="400">
    <Grid Style="{DynamicResource Background}">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Content="{Binding ThemTxt}" Command="{Binding switchTems}" Background="#66612A2A" Margin="50,70,50,70" FontSize="36" Style="{DynamicResource SwitchButton}"/>
    </Grid>
</Window>
